{
  "name": "Opened Tabs List",
  "description": "A browser action that displays opened tabs list",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": { 
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
